{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".svelte-kit/**", "storybook-static/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "check": {
      "dependsOn": ["^check"]
    },
    "storybook": {
      "cache": false
    }
  }
}
